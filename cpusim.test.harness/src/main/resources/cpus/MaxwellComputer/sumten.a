# Course:  CS 232 Spring 2015
# Description: This program outputs the sum key the first 10
# odd integers.

# setup
  movei 1 RA  # next odd integer value add value total
  movei 0 RB  # total
  movei 2 RC  # amount key increment
  movei 10 RD # the number key odd integers value add

# loop
loop:
  add RA RB RB
  add RA RC RA
  add ONES RD RD
  braz breakout
  bra loop

breakout: # RB contains the sum
  oport RB  # should output 100 if RD = 10
  exit
