<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx/8">
    <TitledPane fx:id="titledPane" collapsible="false" animated="false"
                contentDisplay="TOP" minWidth="0.0" text="untitled">
        <TableView id="regTable" fx:id="table" editable="true">
            <columns>
                <TableColumn text="Addr" fx:id="address" editable="false"
                             sortable="false"/>
                <TableColumn text="Data" fx:id="data" sortable="false"/>
            </columns>
        </TableView>
    </TitledPane>
</AnchorPane>
