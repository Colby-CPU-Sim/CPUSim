<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<fx:root id="controlsBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="40.0" prefHeight="40.0" spacing="15.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@../css/Dialogs.css" />
   </stylesheets>
   <children>
      <Button fx:id="newButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onNewButtonClick" text="New" textOverrun="WORD_ELLIPSIS" HBox.hgrow="ALWAYS">
         <graphic>
            <FontIcon iconLiteral="fa-plus" />
         </graphic>
      </Button>
      <Button fx:id="deleteButton" disable="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Delete" HBox.hgrow="ALWAYS">
         <graphic>
            <FontIcon iconLiteral="fa-times" />
         </graphic>
      </Button>
      <Button fx:id="duplicateButton" disable="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onDuplicateButtonClick" text="Duplicate" HBox.hgrow="ALWAYS">
         <graphic>
            <FontIcon iconLiteral="fa-files-o" />
         </graphic>
      </Button>
      <Button fx:id="propertiesButton" disable="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onPropertiesButtonClick" text="Properties" HBox.hgrow="ALWAYS">
        <graphic>
           <FontIcon iconLiteral="fa-ellipsis-h" />
        </graphic>
      </Button>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>


   <stylesheets>
      <URL value="@../css/Dialogs.css" />
   </stylesheets>
</fx:root>
